{
  "author": "swapdk-0x",
  "dependencies": {
    "@near-js/providers": "~2.5.0",
    "@swapdk/contracts": "workspace:*",
    "@swapdk/tokens": "workspace:*",
    "@swapdk/types": "workspace:*",
    "ethers": "^6.14.0",
    "ts-pattern": "5.9.0",
    "zod": "3.25.74",
    "zustand": "5.0.8"
  },
  "description": "SwapDK - Helpers",
  "devDependencies": {
    "@near-js/providers": "2.5.0",
    "@swapdk/toolboxes": "workspace:*",
    "ethers": "6.15.0",
    "tronweb": "6.1.0"
  },
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "default": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./api": {
      "bun": "./src/api/index.ts",
      "default": "./dist/api/index.js",
      "require": "./dist/api/index.cjs",
      "types": "./dist/types/api/index.d.ts"
    },
    "./contracts": {
      "bun": "./src/contracts.ts",
      "default": "./dist/contracts.js",
      "require": "./dist/contracts.cjs",
      "types": "./dist/types/contracts.d.ts"
    },
    "./tokens": {
      "bun": "./src/tokens.ts",
      "default": "./dist/tokens.js",
      "require": "./dist/tokens.cjs",
      "types": "./dist/types/tokens.d.ts"
    }
  },
  "files": ["dist/", "src/"],
  "homepage": "https://github.com/Swap-DK/SwapDK",
  "license": "Apache-2.0",
  "name": "@swapdk/helpers",
  "repository": { "directory": "packages/helpers", "type": "git", "url": "git+https://github.com/Swap-DK/SwapDK.git" },
  "scripts": {
    "build": "bun run ./build.ts",
    "build:clean": "rm -rf dist && bun run ./build.ts",
    "clean": "rm -rf dist node_modules *.tsbuildinfo",
    "generate-tokens": "bun --bun ./fetchTokenLists.ts",
    "test": "bun test",
    "test:ci": "bun test --coverage",
    "type-check": "bun tsc --noEmit",
    "type-check:go": "tsgo"
  },
  "type": "module",
  "version": "4.5.9"
}
