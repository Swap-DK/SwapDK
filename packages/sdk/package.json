{
  "author": "swapdk-0x",
  "dependencies": {
    "@swapdk/core": "workspace:*",
    "@swapdk/helpers": "workspace:*",
    "@swapdk/plugins": "workspace:*",
    "@swapdk/server": "workspace:*",
    "@swapdk/toolboxes": "workspace:*",
    "@swapdk/wallets": "workspace:*"
  },
  "description": "SwapDK - SDK",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./core": {
      "bun": "./src/core.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs",
      "types": "./dist/types/core.d.ts"
    },
    "./plugins": {
      "bun": "./src/plugins.ts",
      "import": "./dist/plugins.js",
      "require": "./dist/plugins.cjs",
      "types": "./dist/types/plugins.d.ts"
    },
    "./server": {
      "bun": "./src/server.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs",
      "types": "./dist/types/server.d.ts"
    },
    "./wallets": {
      "bun": "./src/wallets.ts",
      "import": "./dist/wallets.js",
      "require": "./dist/wallets.cjs",
      "types": "./dist/types/wallets.d.ts"
    }
  },
  "files": ["dist/", "src/"],
  "homepage": "https://github.com/Swap-DK/SwapDK",
  "license": "Apache-2.0",
  "name": "@swapdk/sdk",
  "repository": { "directory": "packages/sdk", "type": "git", "url": "git+https://github.com/Swap-DK/SwapDK.git" },
  "scripts": {
    "build": "bun run ./build.ts",
    "build:clean": "rm -rf dist && bun run ./build.ts",
    "clean": "rm -rf dist node_modules *.tsbuildinfo",
    "type-check": "bun tsc --noEmit",
    "type-check:go": "tsgo"
  },
  "type": "module",
  "version": "4.2.11"
}
