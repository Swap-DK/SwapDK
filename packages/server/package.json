{
  "author": "swapdk-0x",
  "dependencies": {
    "@swapdk/helpers": "workspace:*",
    "@swapdk/tokens": "workspace:*",
    "@swapdk/toolboxes": "workspace:*",
    "@swapdk/wallet-keystore": "workspace:*"
  },
  "description": "SwapDK - Server SDK",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": ["dist/", "src/"],
  "homepage": "https://github.com/Swap-DK/SwapDK",
  "license": "Apache-2.0",
  "name": "@swapdk/server",
  "repository": { "directory": "packages/server", "type": "git", "url": "git+https://github.com/Swap-DK/SwapDK.git" },
  "scripts": {
    "build": "bun run ./build.ts",
    "build:clean": "rm -rf dist && bun run ./build.ts",
    "clean": "rm -rf dist node_modules *.tsbuildinfo",
    "test": "echo 'bun test'",
    "test:ci": "echo 'bun test --coverage'",
    "type-check": "bun tsc --noEmit",
    "type-check:go": "tsgo"
  },
  "type": "module",
  "version": "4.0.54"
}
